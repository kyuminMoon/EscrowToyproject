{
  "name": "payment-front-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite -- --mode local",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "esbuild": "^0.15.8",
    "file-saver": "^2.0.5",
    "streamsaver": "^2.0.5",
    "v-calendar": "^3.0.0-alpha.8",
    "v-pagination-3": "^0.1.7",
    "vue": "^3.2.33",
    "vue-intl": "^6.2.2",
    "vue-loaders": "^4.1.4",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@formatjs/cli": "^4.8.4",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/test-utils": "^2.0.0-rc.6",
    "babel-jest": "^26.6.3",
    "cypress": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jsdom": "^16.5.3",
    "lint-staged": "^10.5.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.7.3",
    "vite": "^2.9.8",
    "vue-clipboard3": "^1.0.1",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-tsc": "^0.34.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --fix",
    "cypress/**/*.js": "eslint --fix"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {}
    },
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,vue}",
      "!<rootDir>/src/config.ts"
    ],
    "moduleFileExtensions": [
      "vue",
      "ts",
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-test.ts"
    ]
  }
}
